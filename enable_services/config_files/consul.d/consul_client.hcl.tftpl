ui = true
log_level = "INFO"
data_dir = "/opt/consul/data"
bind_addr = "0.0.0.0"
client_addr = "0.0.0.0"
advertise_addr = "${lan_ip}"
retry_join = ${retry_join}

encrypt = "${gossip_key}"

acl {
    enabled = true
    default_policy = "deny"
    down_policy = "extend-cache"
}

connect {
  enabled = true
}
ports {
  grpc = 8502
}